create table tbl_position (
	num int AUTO_INCREMENT primary key,
	major int,
	minor int,
	currenttime date,
	staytime int
);

CREATE TABLE `user` (
	`user_id` VARCHAR(50) NOT NULL,
	`user_pw` VARCHAR(50) NULL DEFAULT NULL,
	PRIMARY KEY (`user_id`)
)

CREATE TABLE `loc_info` (
   `user_id` VARCHAR(50) NULL DEFAULT NULL,
   `user_pw` VARCHAR(50) NULL DEFAULT NULL
);