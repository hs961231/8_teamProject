create database 3MS
use 3MS

create table position (
   major int,
   minor int,
   user_id varchar(50),
   current_timedate timestamp,
   stay_time int
);

create table user(
   user_id varchar(50) primary key,
   user_pw varchar(50),
   age int,
   gender varchar(2)
);

create table coupon (
   coupon_id int auto_increment primary key,
   coupon_name varchar(50),
   coupon_content varchar(300)
);

create table tile(
   major int,
   minor int,
   tile_name varchar(50)
)

create table event(
	e_id int auto_increment primary key,
	e_name varchar(100),
	e_start date,
	e_end date
);

create table couponBasket(

user_id varchar(50),
coupon_id varchar(50)

)
